{
  "name": "mine.js",
  "version": "0.0.2",
  "description": "node.JS implementation of a OpenMined data mine",
  "main": "lib/mines.js",
  "private": true,
  "bin": {
    "mine": "bin/cli.js"
  },
  "scripts": {
    "test": "standard && nyc ava",
    "test:coverage": "nyc ava",
    "test:watch": "ava --watch",
    "env-start": "docker-compose -f setup/docker-compose.yml up -d",
    "env-stop": "docker-compose -f setup/docker-compose.yml stop",
    "start": "nodemon ./bin/cli.js --mine-address 0xb8ce9ab6943e0eced004cde8e3bbed6568b2fa01 --contract-address 0xb8ce9ab6943e0eced004cde8e3bbed6568b2fa01",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenMined/mine.js.git"
  },
  "engine": {
    "node": "^8.0.0",
    "npm": ">5.0.0"
  },
  "keywords": [
    "open-mined",
    "privacy",
    "blockchain",
    "data-mining"
  ],
  "bin": {
    "mine": "cli.js"
  },
  "author": "Andreas Offenhaeuser <http://anoff.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OpenMined/mine.js/issues"
  },
  "homepage": "https://github.com/OpenMined/mine.js#readme",
  "devDependencies": {
    "ava": "^0.21.0",
    "codecov": "^2.3.0",
    "coveralls": "^2.13.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "standard": "^10.0.2",
    "testdouble": "^3.2.4"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "ipfs-api": "^14.1.1",
    "node-schedule": "^1.2.3",
    "tmp": "0.0.31",
    "web3": "^1.0.0-beta.14"
  },
  "standard": {
    "globals": [
      "config"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
